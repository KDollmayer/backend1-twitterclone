<%- include("partials/header.ejs") %>

    <h1 class="text-center p-5">Create new account</h1>
    <form method="POST" class="">
        <div class="row justify-content-center">
            <div class="col-5 mb-3">
                 First name:<input type="text" class="form-control" name="firstname">
            </div>
            
        </div>
        <div class="row justify-content-center">
            <div class="col-5 mb-3">
                Last name:<input class="form-control" type="text" name="lastname">
            </div>
        </div>
        
        <div class="row justify-content-center">
            <div class="col-5 mb-3">E-mail:<input class="form-control" type="email" name="email"></div>
            

        </div>
        <div class="row justify-content-center">
            <div class="col-5 mb-3">Username: <input class="form-control" type="text" name="username" required></div>
        </div>
        <div class="row justify-content-center">
            <div class=" col-5 mb-3">Password: <input class="form-control" type="password" name="password" required></div>
            
        </div>
        <div class="row-5 justify-content-center text-center">
            <button class="btn btn-primary rounded-pill " type="submit">Create user</button>
            
        </div>
        
       

    </form>


    <%- include("partials/footer.ejs") %>